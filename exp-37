#include <stdio.h>

void printFactors(int n) {
    printf("Factors of %d are: ", n);
    for (int i = 1; i <= n; i++) {
        if (n % i == 0) {
            printf("%d ", i);
        }
    }
    printf("\n");
}

int main() {
    int n;

    // Input the number
    printf("Enter a positive integer: ");
    scanf("%d", &n);

    // Print factors of the number
    printFactors(n);

    return 0;
}
